{
	"name": "Web Sequence",
	"version": "1.0.21",
	"manifest_version": 2,
	"description": "Real-time & offline tool for generating sequence diagrams",
	"homepage_url": "http://zenuml.com",
	"permissions": ["storage", "tabs", "<all_urls>"],
	"optional_permissions": ["downloads"],
	"content_security_policy": "script-src 'self' filesystem: https://www.google-analytics.com https://www.googletagmanager.com 'unsafe-eval'; object-src 'self'",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"browser_action": {
		"default_title": "Start Web Sequence",
		"default_icon": "icon-48.png"
	},
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png"
	}
}
